    <script src="../../scripts/jquery-3.4.1.slim.min.js"></script>
    <script>
    if (typeof jQuery != 'undefined') { 
        $(".check-button").on("click", (e) => {
            let allCorrect = true;
            $(e.target).parents("div").find("input, select").each((i, el) => {
                let models = $(el).attr("model").split(",");
                let matched = false;
                let keepspace = $(el)[0].hasAttribute("keepspace");
                jQuery.each(models, (i, model) => {
                    let answer = $(el).val();
                    answer = answer.trim();
                    if (!keepspace) answer = answer.replace(/\s/g,'')
                    if (answer == model) matched = true;
                });
                $(el).removeClass("wrong");
                if (!matched) {
                    allCorrect = false;
                    $(el).addClass("wrong");
                }
            });
            let result = $(e.target).next(".check-result");
            if (allCorrect) {
                result.html("Correct!");
                result.addClass("correct");
                result.removeClass("wrong");
            }
            else {
                result.html("Wrong!");
                result.addClass("wrong");
                result.removeClass("correct");
            }
        });
        $(".show-button").on("click", (e) => {
            if ($(e.target).hasClass("hide-button")) {
                $(e.target).parent().next(".answer-area").hide();
                $(e.target).text("Click here to show the correct answer and video explanation");
                $(e.target).parent().prevAll("pre").removeClass("selectable");
            }
            else {
                $(e.target).parent().next(".answer-area").show();
                $(e.target).text("Click here to hide the correct answer and video explanation");
                $(e.target).parent().prevAll("pre").addClass("selectable");
            }			
            $(e.target).toggleClass("hide-button");
        });
        $("pre").each((i, el) => {
            if ($(el).attr("src")) {
                $(el).html(atob($(el).attr("src")));
            }
        });
    }
    </script>

</body></html>

