{
  "name": "hkust-course-backup",
  "homepage": "https://hkleungai.github.io/HKUST-Course-Backup",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "live-server src",
    "link-alias": "yarn tsc-alias --dir src/script",
    "build": "tsc && yarn link-alias",
    "build:watch": "concurrently \"tsc --watch\" \"yarn link-alias --watch\"",
    "clean": "rm src/script/**/*.js",
    "predeploy": "yarn build",
    "deploy": "cross-env NODE_DEBUG=gh-pages gh-pages -d src"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "live-server": "1.2.1",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "typescript": "^4.6.4"
  }
}
